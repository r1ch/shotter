och": { "from": 903363, "to": 907534 }, "speech": [ "Look, I told you before, kid,", "don't bother me. Now beat it." ], "tokens": { "Kid": { "global": 10, "local": 1 } }, "switchCount": 9 }, { "lineNumber": 237, "timeText": "00:17:06,986 --> 00:17:09,030", "timeEpoch": { "from": 1026986, "to": 1029030 }, "speech": [ "You kids are in coach.", "We're up here." ], "tokens": { "Kid": { "global": 11, "local": 1 } }, "switchCount": 9 }, { "lineNumber": 262, "timeText": "00:19:11,277 --> 00:19:14,656", "timeEpoch": { "from": 1151277, "to": 1154656 }, "speech": [ "Don't you feel like a heel flying first", "class with the kids back in coach?" ], "tokens": { "Kid": { "global": 12, "local": 1 } }, "switchCount": 9 }, { "lineNumber": 263, "timeText": "00:19:14,781 --> 00:19:16,658", "timeEpoch": { "from": 1154781, "to": 1156658 }, "speech": [ "No, the kids are fine." ], "tokens": { "Kid": { "global": 13, "local": 1 } }, "switchCount": 9 }, { "lineNumber": 264, "timeText": "00:19:16,783 --> 00:19:20,912", "timeEpoch": { "from": 1156783, "to": 1160912 }, "speech": [ "Only flying I ever did as a kid was in the", "family station wagon, and it wasn't to France." ], "tokens": { "Kid": { "global": 14, "local": 1 } }, "switchCount": 9 }, { "lineNumber": 265, "timeText": "00:19:20,995 --> 00:19:24,332", "timeEpoch": { "from": 1160995, "to": 1164332 }, "speech": [ "We used to have to go over", "to Aunt Laura and Uncle Arthur's house." ], "tokens": { "Home Alone": { "global": 10, "local": 1 } }, "isSwitch": true, "switchCount": 10 }, { "lineNumber": 266, "timeText": "00:19:24,457 --> 00:19:26,738", "timeEpoch": { "from": 1164457, "to": 1166738 }, "speech": [ "The kids are okay. They're", "having the time of their lives." ], "tokens": { "Kid": { "global": 15, "local": 1 } }, "switchCount": 10 }, { "lineNumber": 285, "timeText": "00:21:34,879 --> 00:21:37,048", "timeEpoch": { "from": 1294879, "to": 1297048 }, "speech": [ "Kevin, you're completely helpless." ], "tokens": { "Kevin": { "global