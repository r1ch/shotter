, "switchCount": 5 }, { "lineNumber": 1189, "timeText": "01:47:55,427 --> 01:48:00,223", "timeEpoch": { "from": 6475427, "to": 6480223 }, "speech": [ "And Voldemort was there,", "only he wasn't quite human." ], "tokens": { "Voldemort": { "global": 6, "local": 1 } }, "isSwitch": true, "switchCount": 6 }, { "lineNumber": 1277, "timeText": "02:01:27,697 --> 02:01:30,699", "timeEpoch": { "from": 7287697, "to": 7290699 }, "speech": [ "The Dark Lord shall rise..." ], "tokens": { "Ron": { "global": 5, "local": 1 } }, "switchCount": 6 }, { "lineNumber": 1294, "timeText": "02:04:16,991 --> 02:04:21,829", "timeEpoch": { "from": 7456991, "to": 7461829 }, "speech": [ "My Lord, had I detected any sign,", "a whisper of your whereabouts--" ], "tokens": { "Ron": { "global": 6, "local": 1 } }, "switchCount": 6 }, { "lineNumber": 1296, "timeText": "02:04:25,166 --> 02:04:30,337", "timeEpoch": { "from": 7465166, "to": 7470337 }, "speech": [ "I assure you, my Lord,", "I have never renounced the old ways." ], "tokens": { "Ron": { "global": 7, "local": 1 } }, "switchCount": 6 }, { "lineNumber": 1346, "timeText": "02:07:44,032 --> 02:07:47,784", "timeEpoch": { "from": 7664032, "to": 7667784 }, "speech": [ "And I, being a merciful Lord..." ], "tokens": { "Ron": { "global": 8, "local": 1 } }, "switchCount": 6 }, { "lineNumber": 1371, "timeText": "02:10:32,575 --> 02:10:34,576", "timeEpoch": { "from": 7832575, "to": 7834576 }, "speech": [ "Voldemort's back." ], "tokens": { "Voldemort": { "global": 7, "local": 1 } }, "isSwitch": true, "switchCount": 7 }, { "lineNumber": 1397, "timeText": "02:12:48,336 --> 02:12:50,086", "timeEpoch": { "from": 7968336, "to": 7970086 }, "speech": [ "The Dark Lord." ], "tokens": { "Ron": { "global": 9, "local": 1 } }, "switchCount": 7 }, { "lineNumber": 1419, "timeText": "02:14:47,788 --> 02:14:52,125", "timeEpoch": { "from": 8087788, "to": 8092125 }, "speech": [ "The blood that runs through these veins", "runs within the Dark Lord." ], "tokens": { "Ron": { "glo