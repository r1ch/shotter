{ "lines": [ { "lineNumber": 9, "timeText": "00:03:15,178 --> 00:03:16,429", "timeEpoch": { "from": 195178, "to": 196429 }, "speech": [ "<font color=\"#696969\">\"Harry Potter.\"</font>" ], "tokens": { "Harry": { "global": 1, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 10, "timeText": "00:03:16,846 --> 00:03:18,514", "timeEpoch": { "from": 196846, "to": 198514 }, "speech": [ "<font color=\"#696969\">Who's Harry Potter?</font>" ], "tokens": { "Harry": { "global": 2, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 17, "timeText": "00:03:45,414 --> 00:03:48,229", "timeEpoch": { "from": 225414, "to": 228229 }, "speech": [ "<font color=\"#696969\">You can tell me all about", "that tosser Harry Potter.</font>" ], "tokens": { "Harry": { "global": 3, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 18, "timeText": "00:04:39,110 --> 00:04:41,299", "timeEpoch": { "from": 279110, "to": 281299 }, "speech": [ "<font color=\"#696969\">You've been reckless this summer, Harry.</font>" ], "tokens": { "Harry": { "global": 4, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 31, "timeText": "00:05:33,536 --> 00:05:37,810", "timeEpoch": { "from": 333536, "to": 337810 }, "speech": [ "<font color=\"#696969\">Harry, I assume, right about now, you're wondering why I brought you here.</font>" ], "tokens": { "Harry": { "global": 5, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 34, "timeText": "00:05:47,820 --> 00:05:49,696", "timeEpoch": { "from": 347820, "to": 349696 }, "speech": [ "<font color=\"#696969\">Wands out, Harry.</font>" ], "tokens": { "Harry": { "global": 6, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 38, "timeText": "00:07:10,814 --> 00:07:13,107", "timeEpoch": { "from": 430814, "to": 433107 }, "speech": [ "<font color=\"#696969\">No need to disfigure me, Albus.</font>" ], "tokens": { "Dumbledore": { "global": 1, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 43, "timeText": "00:07:28,851 --> 00:07:32,917", "timeEpoch": { "from": 448851, "to": 452917 }, "speech": [ "<font color=\"#696969\">Harry, I'd like you to meet an old friend and colleague of mine....</font>" ], "tokens": { "Harry": { "global": 7, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 47, "timeText": "00:07:39,695 --> 00:07:41,988", "timeEpoch": { "from": 459695, "to": 461988 }, "speech": [ "<font color=\"#696969\">Harry Potter.</font>" ], "tokens": { "Harry": { "global": 8, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 58, "timeText": "00:08:50,594 --> 00:08:53,200", "timeEpoch": { "from": 530594, "to": 533200 }, "speech": [ "<font color=\"#696969\">Don't think I don't know", "why you're here, Albus.</font>" ], "tokens": { "Dumbledore": { "global": 2, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 88, "timeText": "00:10:48,725 --> 00:10:51,748", "timeEpoch": { "from": 648725, "to": 651748 }, "speech": [ "<font color=\"#696969\">You're like my friend Mr. Potter here, one of a kind.</font>" ], "tokens": { "Harry": { "global": 9, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 97, "timeText": "00:11:33,245 --> 00:11:36,686", "timeEpoch": { "from": 693245, "to": 696686 }, "speech": [ "<font color=\"#696969\">Professor Slughorn is gonna try to collect you, Harry.</font>" ], "tokens": { "Harry": { "global": 10, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 100, "timeText": "00:11:45,652 --> 00:11:48,780", "timeEpoch": { "from": 705652, "to": 708780 }, "speech": [ "<font color=\"#696969\">I fear I may have stolen", "a wondrous night from you, Harry.</font>" ], "tokens": { "Harry": { "global": 11, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 103, "timeText": "00:11:57,747 --> 00:12:01,396", "timeEpoch": { "from": 717747, "to": 721396 }, "speech": [ "<font color=\"#696969\">Oh, you'll not be returning to Little Whinging tonight, Harry.</font>" ], "tokens": { "Harry": { "global": 12, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 109, "timeText": "00:12:49,462 --> 00:12:51,756", "timeEpoch": { "from": 769462, "to": 771756 }, "speech": [ "<font color=\"#696969\">I was only wondering", "when Harry got here.</font>" ], "tokens": { "Harry": { "global": 13, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 110, "timeText": "00:12:51,860 --> 00:12:55,092", "timeEpoch": { "from": 771860, "to": 775092 }, "speech": [ "<font color=\"#696969\">~ What? Harry? Harry who?", "~ Harry Potter, of course.</font>" ], "tokens": { "Harry": { "global": 16, "local": 3 } }, "switchCount": 0 }, { "lineNumber": 111, "timeText": "00:12:55,301 --> 00:12:58,429", "timeEpoch": { "from": 775301, "to": 778429 }, "speech": [ "<font color=\"#696969\">I think I'd know if Harry Potter was in my house, wouldn't I?</font>" ], "tokens": { "Harry": { "global": 17, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 113, "timeText": "00:13:04,789 --> 00:13:06,457", "timeEpoch": { "from": 784789, "to": 786457 }, "speech": [ "<font color=\"#696969\">Harry? Did someone say \"Harry\"?</font>" ], "tokens": { "Harry": { "global": 19, "local": 2 } }, "switchCount": 0 }, { "lineNumber": 119, "timeText": "00:13:18,551 --> 00:13:20,115", "timeEpoch": { "from": 798551, "to": 800115 }, "speech": [ "<font color=\"#696969\">Harry!</font>" ], "tokens": { "Harry": { "global": 20, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 120, "timeText": "00:13:26,371 --> 00:13:28,039", "timeEpoch": { "from": 806371, "to": 808039 }, "speech": [ "<font color=\"#696969\">Harry!</font>" ], "tokens": { "Harry": { "global": 21, "local": 1 } }, "switchCount": 0 }, { "lineNumber": 124, "timeText": "00:13:37,840 --> 00:13:40,655", "timeEpoch": { "from": 817840, "to": 820655 }, "speech": [ "<font color=\"#696969\">~ I didn't know. Dumbledore.", "~ Oh, that man.</font>" ], "tokens": { "Dumbledore": { "global": 3, "local": 1 } }, "isSwitch": true, "switchCount": 1 }, { "lineNumber": 137, "timeText": "00:14:24,133 --> 00:14:28,095", "timeEpoch": { "from": 864133, "to": 868095 }, "speech": [ "<font color=\"#696969\">But this is Hogwarts we're talking about. It's Dumbledore. What could be safer?</font>" ], "tokens": { "Dumbledore": { "global": 4, "local": 1 } }, "isSwitch": true, "switchCount": 2 }, { "lineNumber": 139, "timeText": "00:14:32,683 --> 00:14:34,351", "timeEpoch": { "from": 872683, "to": 874351 }, "speech": [ "<font color=\"#696969\">....Dumbledore's got a bit old.</font>" ], "tokens": { "Dumbledore": { "global": 5, "local": 1 } }, "isSwitch": true, "switchCount": 3 }, { "lineNumber": 157, "timeText": "00:16:23,828 --> 00:16:27,790", "timeEpoch": { "from": 983828, "to": 987790 }, "speech": [ "<font color=\"#696969\">Dumbledore is a great wizard. Only a fool would question it.</font>" ], "tokens": { "Dumbledore": { "global": 6, "local": 1 } }, "isSwitch": true, "switchCount": 4 }, { "lineNumber": 204, "timeText": "00:19:59,445 --> 00:20:00,905", "timeEpoch": { "from": 1199445, "to": 1200905 }, "speech": [ "<font color=\"#696969\">Hi, Ron.</font>" ], "tokens": { "Ron": { "global": 1, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 211, "timeText": "00:20:44,070 --> 00:20:45,425", "timeEpoch": { "from": 1244070, "to": 1245425 }, "speech": [ "<font color=\"#696969\">Harry?</font>" ], "tokens": { "Harry": { "global": 22, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 224, "timeText": "00:23:17,963 --> 00:23:19,840", "timeEpoch": { "from": 1397963, "to": 1399840 }, "speech": [ "<font color=\"#696969\">Stop it, Harry.", "I know where you're going.</font>" ], "tokens": { "Harry": { "global": 23, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 227, "timeText": "00:23:24,949 --> 00:23:28,389", "timeEpoch": { "from": 1404949, "to": 1408389 }, "speech": [ "<font color=\"#696969\">Harry is under the impression Draco Malfoy is now a Death Eater.</font>" ], "tokens": { "Harry": { "global": 24, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 233, "timeText": "00:23:43,299 --> 00:23:44,863", "timeEpoch": { "from": 1423299, "to": 1424863 }, "speech": [ "<font color=\"#696969\">Hermione saw it with her own eyes.</font>" ], "tokens": { "Hermione": { "global": 1, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 248, "timeText": "00:25:42,785 --> 00:25:44,036", "timeEpoch": { "from": 1542785, "to": 1544036 }, "speech": [ "<font color=\"#696969\">Where's Harry?</font>" ], "tokens": { "Harry": { "global": 25, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 250, "timeText": "00:26:02,908 --> 00:26:06,349", "timeEpoch": { "from": 1562908, "to": 1566349 }, "speech": [ "<font color=\"#696969\">Didn't Mummy ever tell you", "it was rude to eavesdrop, Potter?</font>" ], "tokens": { "Harry": { "global": 26, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 256, "timeText": "00:27:24,859 --> 00:27:26,215", "timeEpoch": { "from": 1644859, "to": 1646215 }, "speech": [ "<font color=\"#696969\">~ Hello, Harry.", "~ Luna!</font>" ], "tokens": { "Harry": { "global": 27, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 265, "timeText": "00:27:57,598 --> 00:27:59,683", "timeEpoch": { "from": 1677598, "to": 1679683 }, "speech": [ "<font color=\"#696969\">No exceptions, Potter.</font>" ], "tokens": { "Harry": { "global": 28, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 273, "timeText": "00:28:29,920 --> 00:28:31,797", "timeEpoch": { "from": 1709920, "to": 1711797 }, "speech": [ "<font color=\"#696969\">Nice face, Potter.</font>" ], "tokens": { "Harry": { "global": 29, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 325, "timeText": "00:32:19,821 --> 00:32:21,177", "timeEpoch": { "from": 1939821, "to": 1941177 }, "speech": [ "<font color=\"#696969\">Potter.</font>" ], "tokens": { "Harry": { "global": 30, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 337, "timeText": "00:32:58,190 --> 00:33:03,091", "timeEpoch": { "from": 1978190, "to": 1983091 }, "speech": [ "<font color=\"#696969\">Potter, take Weasley with you. He looks far too happy over there.</font>" ], "tokens": { "Ron": { "global": 2, "local": 1 }, "Harry": { "global": 31, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 343, "timeText": "00:33:17,792 --> 00:33:21,754", "timeEpoch": { "from": 1997792, "to": 2001754 }, "speech": [ "<font color=\"#696969\">Harry, my boy, I was beginning to worry. We've brought someone with us, I see.</font>" ], "tokens": { "Harry": { "global": 32, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 344, "timeText": "00:33:22,379 --> 00:33:23,526", "timeEpoch": { "from": 2002379, "to": 2003526 }, "speech": [ "<font color=\"#696969\">Ron Weasley, sir.</font>" ], "tokens": { "Ron": { "global": 3, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 347, "timeText": "00:33:28,948 --> 00:33:32,076", "timeEpoch": { "from": 2008948, "to": 2012076 }, "speech": [ "<font color=\"#696969\">Any friend of Harry's is a friend of mine. Get your books out.</font>" ], "tokens": { "Harry": { "global": 33, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 348, "timeText": "00:33:32,285 --> 00:33:34,787", "timeEpoch": { "from": 2012285, "to": 2014787 }, "speech": [ "<font color=\"#696969\">I haven't actually got", "my book yet, and nor has Ron.</font>" ], "tokens": { "Ron": { "global": 4, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 392, "timeText": "00:38:05,872 --> 00:38:08,583", "timeEpoch": { "from": 2285872, "to": 2288583 }, "speech": [ "<font color=\"#696969\">Harry, you got my message. Come in.</font>" ], "tokens": { "Harry": { "global": 34, "local": 1 } }, "switchCount": 4 }, { "lineNumber": 410, "timeText": "00:38:59,777 --> 00:39:03,634", "timeEpoch": { "from": 2339777, "to": 2343634 }, "speech": [ "<font color=\"#696969\">In this case,", "pertaining to one individual, Voldemort....</font>" ], "tokens": { "Voldemort": { "global": 1, "local": 1 } }, "isSwitch": true, "switchCount": 5 }, { "lineNumber": 415, "timeText": "00:39:57,226 --> 00:40:01,501", "timeEpoch": { "from": 2397226, "to": 2401501 }, "speech": [ "<font color=\"#696969\">I admit some confusion upon receiving your letter, Mr. Dumbledore.</font>" ], "tokens": { "Dumbledore": { "global": 7, "local": 1 } }, "isSwitch": true, "switchCount": 6 }, { "lineNumber": 473, "timeText": "00:45:31,287 --> 00:45:33,476", "timeEpoch": { "from": 2731287, "to": 2733476 }, "speech": [ "<font color=\"#696969\">No hard feelings, Weasley, all right?</font>" ], "tokens": { "Ron": { "global": 5, "local": 1 } }, "switchCount": 6 }, { "lineNumber": 482, "timeText": "00:46:10,907 --> 00:46:12,575", "timeEpoch": { "from": 2770907, "to": 2772575 }, "speech": [ "<font color=\"#696969\">Come on, Weasley!</font>" ], "tokens": { "Ron": { "global": 6, "local": 1 } }, "switchCount": 6 }, { "lineNumber": 483, "timeText": "00:46:12,784 --> 00:46:14,452", "timeEpoch": { "from": 2772784, "to": 2774452 }, "speech": [ "<font color=\"#696969\">~ Come on, Ron!", "~ Go on, Weasley!</font>" ], "tokens": { "Ron": { "global": 8, "local": 2 } }, "switchCount": 6 }, { "lineNumber": 484, "timeText": "00:46:16,016 --> 00:46:18,935", "timeEpoch": { "from": 2776016, "to": 2778935 }, "speech": [ "<font color=\"#696969\">~ Go on, Weasley!", "~ Yeah, Ron!</font>" ], "tokens": { "Ron": { "global": 10, "local": 2 } }, "switchCount": 6 }, { "lineNumber": 486, "timeText": "00:46:36,869 --> 00:46:38,954", "timeEpoch": { "from": 2796869, "to": 2798954 }, "speech": [ "<font color=\"#696969\">Come on, Ron!</font>" ], "tokens": { "Ron": { "global": 11, "local": 1 } }, "switchCount": 6 }, { "lineNumber": 487, "timeText": "00:46:47,191 --> 00:46:48,859", "timeEpoch": { "from": 2807191, "to": 2808859 }, "speech": [ "<font color=\"#696969\">Come on, Ron.</font>" ], "tokens": { "Ron": { "global": 12, "local": 1 } }, "switchCount": 6 }, { "lineNumber": 488, "timeText": "00:46:49,589 --> 00:46:51,465", "timeEpoch": { "from": 2809589, "to": 2811465 }, "speech": [ "<font color=\"#696969\">Come on, Ron.</font>" ], "tokens": { "Ron": { "global": 13, "local": 1 } }, "switchCount": 6 }, { "lineNumber": 493, "timeText": "00:47:38,801 --> 00:47:41,825", "timeEpoch": { "from": 2858801, "to": 2861825 },